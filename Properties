		private static void place2() throws IOException {
			
			System.out.println("[Purple]\nMediterranean Avenue\nPrice: $60\nWould you like to buy this property?\n1. Yes\n2. No");
			userInput = in.readLine();
			userInt = Integer.parseInt(userInput);
			
			if(userInt == 1) {
				System.out.println("You bought Mediterranean Avenue for $60");
			}
			
			else if(userInt == 2) {
				System.out.println(" ");
			}
		}
		
